{
    "name": "lieroes/venipak-sdk",
    "description": "Venipak SDK for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["php", "delivery", "venipak"],
    "authors": [
        {
            "name": "Olegs Davidovs",
            "email": "davidovs.olegs@gmail.com",
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "VenipakSDK\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VenipakSDK\\VenipakServiceProvider"
            ],
            "aliases": {
                "Venipak": "VenipakSDK\\Facades\\Venipak"
            }
        }
    }
}