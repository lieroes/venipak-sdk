{
    "name": "lieroes/venipak-sdk",
    "description": "Venipak SDK for Laravel",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "VenipakSDK\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VenipakSDK\\VenipakServiceProvider"
            ],
            "aliases": {
                "Venipak": "VenipakSDK\\Facades\\Venipak"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}